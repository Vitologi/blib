.bJsonEditor{}

.bJsonEditor h1,
.bJsonEditor h2,
.bJsonEditor h3,
.bJsonEditor h4,
.bJsonEditor h5,
.bJsonEditor h6 {
    margin: 10px 0;
    font-family: inherit;
    font-weight: bold;
    line-height: 20px;
    color: inherit;
    text-rendering: optimizelegibility;
}

.bJsonEditor .btn-group {
    position: relative;
    display: inline-block;
    font-size: 0;
    vertical-align: middle;
    white-space: nowrap;}

.bJsonEditor .btn {
    display: inline-block;
    padding: 4px 12px;
    margin-bottom: 0;
    font-size: 14px;
    line-height: 20px;
    text-align: center;
    vertical-align: middle;
    cursor: pointer;
    color: #333333;
    text-shadow: 0 1px 1px rgba(255, 255, 255, 0.75);
    background-color: #f5f5f5;
    background-image: -moz-linear-gradient(top, #ffffff, #e6e6e6);
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#ffffff), to(#e6e6e6));
    background-image: -webkit-linear-gradient(top, #ffffff, #e6e6e6);
    background-image: -o-linear-gradient(top, #ffffff, #e6e6e6);
    background-image: linear-gradient(to bottom, #ffffff, #e6e6e6);
    background-repeat: repeat-x;
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffffffff', endColorstr='#ffe6e6e6', GradientType=0);
    border-color: #e6e6e6 #e6e6e6 #bfbfbf;
    border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
    filter: progid:DXImageTransform.Microsoft.gradient(enabled = false);
    border: 1px solid #cccccc;
    border-bottom-color: #b3b3b3;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    -webkit-box-shadow: inset 0 1px 0 rgba(255,255,255,.2), 0 1px 2px rgba(0,0,0,.05);
    -moz-box-shadow: inset 0 1px 0 rgba(255,255,255,.2), 0 1px 2px rgba(0,0,0,.05);
    box-shadow: inset 0 1px 0 rgba(255,255,255,.2), 0 1px 2px rgba(0,0,0,.05);
}

.bJsonEditor .well {
    min-height: 20px;
    padding: 19px;
    margin-bottom: 20px;
    background-color: #f5f5f5;
    border: 1px solid #e3e3e3;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.05);
    -moz-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.05);
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.05);
}

.bJsonEditor .well-small {
    padding: 9px;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
}

.bJsonEditor .container-fluid {
    padding-right: 20px;
    padding-left: 20px;
}

.bJsonEditor .row-fluid {
    width: 100%;
    margin-bottom: 10px;
}

.bJsonEditor .control-group {
    margin-bottom: 10px;
}

.bJsonEditor select,
.bJsonEditor textarea,
.bJsonEditor input[type="text"],
.bJsonEditor input[type="password"],
.bJsonEditor input[type="datetime"],
.bJsonEditor input[type="datetime-local"],
.bJsonEditor input[type="date"],
.bJsonEditor input[type="month"],
.bJsonEditor input[type="time"],
.bJsonEditor input[type="week"],
.bJsonEditor input[type="number"],
.bJsonEditor input[type="email"],
.bJsonEditor input[type="url"],
.bJsonEditor input[type="search"],
.bJsonEditor input[type="tel"],
.bJsonEditor input[type="color"],
.bJsonEditor .uneditable-input {
    display: inline-block;
    width: 98%;
    padding: 4px 1% 4px 1%;
    margin-bottom: 10px;
    font-size: 14px;
    line-height: 20px;
    color: #555555;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    vertical-align: middle;
}

.bJsonEditor textarea,
.bJsonEditor input[type="text"],
.bJsonEditor input[type="password"],
.bJsonEditor input[type="datetime"],
.bJsonEditor input[type="datetime-local"],
.bJsonEditor input[type="date"],
.bJsonEditor input[type="month"],
.bJsonEditor input[type="time"],
.bJsonEditor input[type="week"],
.bJsonEditor input[type="number"],
.bJsonEditor input[type="email"],
.bJsonEditor input[type="url"],
.bJsonEditor input[type="search"],
.bJsonEditor input[type="tel"],
.bJsonEditor input[type="color"],
.bJsonEditor .uneditable-input {
    background-color: #ffffff;
    border: 1px solid #cccccc;
    -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
    -moz-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
    -webkit-transition: border linear .2s, box-shadow linear .2s;
    -moz-transition: border linear .2s, box-shadow linear .2s;
    -o-transition: border linear .2s, box-shadow linear .2s;
    transition: border linear .2s, box-shadow linear .2s;
}

.bJsonEditor .help-inline {
    display: inline-block;
    vertical-align: middle;
    padding-left: 5px;
}

.bJsonEditor .help-block,
.bJsonEditor .help-inline {
    color: #595959;
}

.bJsonEditor input[disabled],
.bJsonEditor select[disabled],
.bJsonEditor textarea[disabled],
.bJsonEditor input[readonly],
.bJsonEditor select[readonly],
.bJsonEditor textarea[readonly] {
    cursor: not-allowed;
    background-color: #eeeeee;
}

.bJsonEditor .table {
    width: 100%;
    margin-bottom: 20px;
}

.bJsonEditor .table-bordered {
    border: 1px solid #dddddd;
    border-collapse: separate;
    border-left: 0;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
}

.bJsonEditor .table thead th {
    vertical-align: bottom;
}

.bJsonEditor .table-bordered th, .table-bordered td {
    border-left: 1px solid #dddddd;
}

.bJsonEditor .table th {
    font-weight: bold;
}

.bJsonEditor .table th, .table td {
    padding: 8px;
    line-height: 20px;
    text-align: left;
    vertical-align: top;
    border-top: 1px solid #dddddd;
}

.bJsonEditor .radio,
.bJsonEditor .checkbox {
    min-height: 20px;
    padding-left: 20px;
}

.bJsonEditor .radio input[type="radio"],
.bJsonEditor .checkbox input[type="checkbox"] {
    float: left;
    margin-left: -20px;
    margin-top: 3px;
}

.bJsonEditor label,
.bJsonEditor input,
.bJsonEditor button,
.bJsonEditor select,
.bJsonEditor textarea {
    font-size: 14px;
    font-weight: normal;
    line-height: 20px;
}